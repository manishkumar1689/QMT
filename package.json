{
  "name": "bundle",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run bundle && ng serve",
    "build": "ng build && npm run bundle",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle": "ng build --prod @eds/angular",
    "publish-bundle:prerelease": "npm run bundle && npm publish dist/eds/angular --tag next",
